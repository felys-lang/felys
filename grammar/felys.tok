ADD: '+' ;
SUB: '-' ;
MUL: '*' ;
DIV: '/' ;
MOD: '%' ;
GE: '>=' ;
LE: '<=' ;
GT: '>' ;
LT: '<' ;
EQ: '==' ;
NE: '!=' ;

@(keyword)
OR: 'or' ;

@(keyword)
AND: 'and' ;

@(keyword)
NOT: 'not' ;

@(keyword)
TRUE: 'true';

@(keyword)
FALSE: 'false';

@(intern, memo, identifier)
IDENT: [a-zA-Z_][a-zA-Z0-9_]* ;

@(ws)
WS: [\u{20}\n\t\r]+ ;

@(ws)
COMMENT: '//' [^\n]* ;
